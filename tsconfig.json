{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "downlevelIteration": true,
    "alwaysStrict": true,
    "jsx": "react",
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "baseUrl": ".",
    "paths": {
      "redux": [
        "node_modules/redux/index.d.ts"
      ],
    },
    "sourceMap": true
  },
  "include": [
    "src"
  ]
}
